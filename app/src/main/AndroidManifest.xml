<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kyubi">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:icon="@drawable/kyubi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KYUBI">
        <activity
            android:name=".Login"
            android:label="Login"
            android:parentActivityName=".Login">
            <meta-data
                android:name="android.support.parent_activity"
                android:value="com.example.kyubi.Login"></meta-data>
        </activity>
        <activity
            android:name=".Registrar"
            android:label="Registrar"
            android:parentActivityName=".Registrar">
            <meta-data
                android:name="android.support.parent_activity"
                android:value="com.example.kyubi.Registrar"></meta-data>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.KYUBI.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>